@page "{id:int?}/{handler?}"
@model AgendaApplication.Pages.TipoContatos.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<br />
<h2>Excluir tipo de categoria</h2>
<hr />

<form method="post">
    <div class="form-border">
        <input type="hidden" asp-for="TipoContato.Id" />
        <fieldset disabled>
            <div class="form-group row">
                <div class="col-sm-2">
                    <label asp-for="TipoContato.Tipo" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="TipoContato.Tipo" class="form-control" />
                </div>
            </div>
        </fieldset>
        <br />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-sm btn-danger" />
            <a asp-page="Index" class="btn btn-sm btn-success"><span class="glyphicon glyphicon-list"></span> De volta à lista </a>
        </div>
    </div>
</form>
